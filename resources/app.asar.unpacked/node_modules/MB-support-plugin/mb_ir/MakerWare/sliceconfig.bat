@echo off

set mw_path=%~d0%~p0

rem developers don't like it when scripts like this ruin their env
set old_path=%PATH%
set old_home=%PYTHONHOME%
set old_pypath=%PYTHONPATH%

rem set up env
set PATH=%mw_path%;%PATH%
set PYTHONHOME=%mw_path%python34
set PYTHONPATH=

rem run sliceconfig
"%mw_path%python3.4.exe" "%mw_path%sliceconfig" %*

rem tear down env
set PATH=%old_path%
set PYTHONHOME=%old_home%
set PYTHONPATH=%old_pypath%
